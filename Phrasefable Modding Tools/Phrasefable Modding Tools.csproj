<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <AssemblyName>Phrasefable_Modding_Tools</AssemblyName>
        <Version>0.0.1</Version>
        <RootNamespace>Phrasefable_Modding_Tools</RootNamespace>
        <TargetFramework>net452</TargetFramework>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="JetBrains.Annotations" Version="2019.1.3" ExcludeAssets="runtime"/>
        <PackageReference Include="Pathoschild.Stardew.ModBuildConfig" Version="3.0.0-beta.8"/>
    </ItemGroup>

    <ItemGroup>
        <_UnmanagedRegistrationCache Remove="obj\Phrasefable Modding Tools.csproj.UnmanagedRegistration.cache"/>
    </ItemGroup>

    <ItemGroup>
        <_ResolveComReferenceCache Remove="obj\x86\Release\net452\Phrasefable Modding Tools.csproj.ResolveComReference.cache"/>
    </ItemGroup>

    <ItemGroup>
        <IntermediateAssembly Remove="obj\x86\Release\net452\Phrasefable_Modding_Tools.dll"/>
    </ItemGroup>

    <ItemGroup>
        <_DebugSymbolsIntermediatePath Remove="obj\x86\Release\net452\Phrasefable_Modding_Tools.pdb"/>
    </ItemGroup>

    <ItemGroup>
        <_DeploymentManifestEntryPoint Remove="obj\x86\Release\net452\Phrasefable_Modding_Tools.dll"/>
    </ItemGroup>

    <ItemGroup>
        <ApplicationManifest Remove="obj\x86\Release\net452\Native.Phrasefable_Modding_Tools.manifest"/>
    </ItemGroup>

    <ItemGroup>
        <DeployManifest Remove="obj\x86\Release\net452\Phrasefable_Modding_Tools.application"/>
    </ItemGroup>

    <ItemGroup>
        <Clean Remove="obj\x86\Release\net452\\TempCA\Phrasefable_Modding_Tools.pdb"/>
        <Clean Remove="obj\x86\Release\net452\\TempCA\Phrasefable_Modding_Tools.dll"/>
    </ItemGroup>

    <Import Project="..\Common\Common.projitems" Label="Shared"/>
</Project>